{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
      "skipBlankLines": "off",
      "no-console": "off",
      "padded-blocks": 0,
      "no-trailing-spaces": 0,
      "no-unused-vars": 1,
      "react/prop-types": "off",
      "arrow-body-style": 1,
      "react/jsx-props-no-spreading": "off",
      "prettier/prettier": "error",
      "no-underscore-dangle": "off"
    },
    "plugins": [
      "prettier"
    ]
}
